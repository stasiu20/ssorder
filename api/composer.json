{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "bentools/webpush-bundle": "^0.7.0",
        "chrisguitarguy/request-id-bundle": "^4.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/rest-bundle": "^3.0",
        "league/fractal": "^0.19.2",
        "lexik/jwt-authentication-bundle": "^2.8",
        "moneyphp/money": "^3.3",
        "nelmio/api-doc-bundle": "^4.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.1",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.4",
        "ergebnis/phpstan-rules": "^0.15.2",
        "escapestudios/symfony2-coding-standard": "^3.11",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpcompatibility/php-compatibility": "^9.3",
        "phploc/phploc": "^7.0",
        "phpmd/phpmd": "^2.9",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.3",
        "symfony/maker-bundle": "^1.22",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/web-profiler-bundle": "^5.3",
        "timeweb/phpstan-enum": "^2.2",
        "wapmorgan/php-code-fixer": "^2.0",
        "zenstruck/foundry": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpcbf": " ./vendor/bin/phpcbf --extensions=php -d memory_limit=800M",
        "phpdd": "./vendor/bin/phpdd ./src/",
        "phpcs": "./vendor/bin/phpcs --report=junit --extensions=php",
        "phpmd": "./vendor/bin/phpmd --ignore-violations-on-exit ./src/ text ./phpmd-ruleset.xml",
        "phploc": "./vendor/bin/phploc ./src/",
        "phpcpd": "./vendor/bin/phpcpd ./src/",
        "phplint": "./vendor/bin/parallel-lint --exclude vendor/ --exclude bin/.phpunit .",
        "phpunit-all": "XDEBUG_MODE=coverage ./bin/phpunit --group default,database --coverage-html ../docs/api --log-junit ../docs/api-junit.xml",
        "all": [
            "@phplint", "@phpdd", "@phpcs", "@phpmd", "@phpcpd", "@phpunit-all"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
